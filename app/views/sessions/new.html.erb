<%= form_for(@user, url: sessions_path ) do |f| %>
  <%= field_set_tag "Login Panel" do %>
	<div class="field">
	    <%= f.label :email %>
		<%= f.text_field :email %>
	</div>
	<div class="field">
	    <%= f.label :password %><%= f.password_field :password %>
	</div>
	<div class="actions">
		<%= f.submit "Login" %>
	</div>
  <% end %>
<% end %>
